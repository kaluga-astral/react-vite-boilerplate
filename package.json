{
  "name": "@astral/react-vite-boilerplate",
  "private": true,
  "scripts": {
    "build:prod": "vite build",
    "dev": "vite",
    "lint:types": "tsc --noEmit",
    "lint": "eslint . --fix --quiet",
    "lint:all": "eslint \"**/*.{ts,tsx}\" --fix --quiet",
